[user]
	name = GIT_USERNAME_PLACEHOLDER
	email = GIT_EMAIL_PLACEHOLDER
[core]
	editor = vim
        whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[alias]
  am = commit --amend
	cb = checkout -b
  co = checkout
	c = commit
	ca = commit --all
  can = commit --all --no-verify
	aa = add --all
	s = status
	last = log -1 HEAD
	bd = branch -d
	pom = push origin master
	po = push origin
	pod = push origin --delete
	pot = push origin --tags
	pl = pull
	plo = pull origin
	d = diff
	m = merge --no-ff
[color]
	ui = auto
[color "status"]
    added = green
    changed = yellow
    untracked = cyan
